You are given two strings s and t.
String t is generated by random shuffling string s and then add one more letter at a random position.
Return the letter that was added to t.
Input: s = "abcd", t = "abcde"
Output: "e"
Explanation: 'e' is the letter that was added.
Input s = "hello"    t = "hlelmo". Find 'm' here.

using System;
using System.Collections.Generic;
using System.Linq;

public class HelloWorld
{
    
    public static void Main(string[] args)
    {
        string s,t;
        Console.WriteLine("Enter a String");
        s=Console.ReadLine();
        
        t=ShuffledString(s);
        Console.WriteLine("Shuffled string: "+t);
        
        addedCharacter1(s,t);
        
    }
    public static void addedCharacter1(string strOriginal,string strShuffled){
        List<char> character=strShuffled.ToList();
        
        List<char> char1=strOriginal.ToList();
        
        var chara=character.Except(char1);
        foreach(char ch in chara)
            Console.WriteLine("Extra character: "+ch);
        
    }
    
    public static string ShuffledString(string str){
        Random random=new Random();
        
        List<char> characters=str.ToList();
        
        for(int i=characters.Count-1;i>0;i--){
            int j=random.Next(i+1);
            
            char temp=characters[i];
            characters[i]=characters[j];
            characters[j]=temp;
        }
        char ch=Convert.ToChar(random.Next(97,123));
        characters.Add(ch);
        return new string(characters.ToArray());
    }
}
